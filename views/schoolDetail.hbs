                 {{!--  <li><a href="/partners"><i class="fa fa-building"></i>Partners</a></li> --}}

                  <li><a href="/partners"><i class="fa fa-building"></i>Partners</a></li>

                  <li><a><i class="fa fa-plus"></i>Add<span class="fa fa-chevron-down"></span></a>
                    <ul class="nav child_menu">
                      <li><a href="/addStudent">Students</a></li>
                      <li><a href="/addCohort">Cohorts</a></li>
                      <li><a href="/addSchool">Schools</a></li>


                      {{!-- <li><a href="/addPartner">Partners</a></li> --}}

                    </ul>
                  </li>
                </ul>
              </div>
            </div>
            <!-- /sidebar menu -->
          </div>
        </div>

        <!-- top navigation -->
        <div class="top_nav">
          <div class="nav_menu">
            <nav>
              <div class="nav toggle">
                <a id="menu_toggle"><i class="fa fa-bars"></i></a>
              </div>

              <ul class="nav navbar-nav navbar-right">
                <li class="">
                  <a href="javascript:;" class="user-profile dropdown-toggle" data-toggle="dropdown" aria-expanded="false">
                    <img src="../../images/{{user.pic}}" alt="">John Doe
                    <span class=" fa fa-angle-down"></span>
                  </a>
                  <ul class="dropdown-menu dropdown-usermenu pull-right">
                    <li><a href="editProfile">Edit Profile</a></li>
                    <li><a href="resetPassword">Reset Password</a></li>
                    <li><a href="logout"><i class="fa fa-sign-out pull-right"></i> Log Out</a></li>
                  </ul>
                </li>
              </ul>
            </nav>
          </div>
        </div>
