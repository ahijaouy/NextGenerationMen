        .col-sm-12
            p
                b Academic Information
    +accordion('grades')
        .panel(class='panel-default')
            .panel-heading(role='tab', id='heading9th')
                h4.panel-title
                    a(data-toggle="collapse", data-parent="accordiongrades",
                      href = '#add9th', aria-expanded = 'true',
                      aria-controls='collapseOne') 9th Grade
            unless expanded
                .panel-collapse.collapse(id="add9th", role='tabpanel'
                                        aria-labelledby="heading9th")
                    .panel-body
                        label Semester 1
                        input
                        label Semester 2
                        input
